@using PL.Web.API
@using Newtonsoft.Json
@using Newtonsoft.Json.Converters
@{ 
    Layout = null;

    var awardAPI = new AwardsAPI();
    Guid guid;
    Guid.TryParse(Request["id"], out guid);
    var award = awardAPI.GetAwardById(guid);

    var json = JsonConvert.SerializeObject(award);

    Response.Write(json); 
}
